  "build": {
    "appId": "com.galaxyxieyu.xhs-runner",
    "productName": "XHS Runner",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**",
      "src/pages/**",
      "package.json",
      "docs/**"
    ],
    "asar": true
  }
